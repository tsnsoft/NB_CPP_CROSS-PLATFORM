${CND_DISTDIR}/${CND_CONF}/${CND_PLATFORM}/nb_cpp_cross-platform: ${OBJECTFILES}
	${MKDIR} -p ${CND_DISTDIR}/${CND_CONF}/${CND_PLATFORM}
	${LINK.cc} -o ${CND_DISTDIR}/${CND_CONF}/${CND_PLATFORM}/nb_cpp_cross-platform ${OBJECTFILES} ${LDLIBSOPTIONS}

${OBJECTDIR}/nb_cpp_cp_demo.o: nb_cpp_cp_demo.cpp
	${MKDIR} -p ${OBJECTDIR}
	${RM} "$@.d"
	$(COMPILE.cc) -O2 -MMD -MP -MF "$@.d" -o ${OBJECTDIR}/nb_cpp_cp_demo.o nb_cpp_cp_demo.cpp

